aamain.f:      use parallel
aamain.f:c module parallel is to specify idproc (needed to print from just proc 0)
aamain.f:      call init_parallel
aamain.f:      use parallel, only : idproc  !for diagnostic printouts from proc 0
aamain.f:      use parallel
aamain.f:      call end_parallel
elem.f:c     describing a parallel-faced bending magnet
elem.f:c  for half of a parallel face bending magnet.
parallel_mod.f:module parallel
parallel_mod.f:      subroutine init_parallel
parallel_mod.f:      end subroutine init_parallel
parallel_mod.f:      subroutine end_parallel
parallel_mod.f:      end subroutine end_parallel
parallel_mod.f:end module parallel
